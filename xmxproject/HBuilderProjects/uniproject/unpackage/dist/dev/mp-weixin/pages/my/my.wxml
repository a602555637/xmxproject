<view><block wx:if="{{name}}"><view class="container"><image class="avatar" src="{{avatarUrl}}"></image><text class="name">{{name}}</text><block wx:if="{{waitTime}}"><view class="waittime">等待期还剩45天</view></block><view class="content"><block wx:if="{{isVip==='2'}}"><image class="phone-vip" src="../../static/my/phoneVIP@2x.png"></image></block><block wx:else><block wx:if="{{isVip==='1'}}"><image class="phone-vip" src="../../static/my/pthy-sj@2x.png"></image></block><block wx:else><image class="phone-vip" src="../../static/my/un-phoneVIP@2x.png"></image></block></block><block wx:if="{{isVip==='2'}}"><image src="../../static/my/padVIP@2x.png"></image></block><block wx:else><block wx:if="{{isVip==='1'}}"><image src="../../static/my/pthy-pb@2x.png"></image></block><block wx:else><image src="../../static/my/un-padVIP@2x.png"></image></block></block></view></view></block><block wx:else><view class="unlogin"><button class="button-class" open-type="getUserInfo" data-event-opts="{{[['tap',[['getUserInfo',['$event']]]]]}}" bindtap="__e"><image src="../../static/my/dl@2x.png"></image></button></view></block><view class="line"></view><uni-list vue-id="1" bind:__l="__l" vue-slots="{{['default']}}"><uni-item vue-id="{{('2')+','+('1')}}" title="我的会员" thumb="../../static/my/icon/vip@2x.png" bind:__l="__l"></uni-item><text class="info-text">了解详情</text><uni-item vue-id="{{('3')+','+('1')}}" title="我的订单" thumb="../../static/my/icon/orders@2x.png" data-event-opts="{{[['^click',[['onOrderList']]]]}}" bind:click="__e" bind:__l="__l"></uni-item><uni-item vue-id="{{('4')+','+('1')}}" title="我的钱包" thumb="../../static/my/icon/wallet@2x.png" data-event-opts="{{[['^click',[['onMyWallet']]]]}}" bind:click="__e" bind:__l="__l"></uni-item><uni-item vue-id="{{('5')+','+('1')}}" title="我的卡包" thumb="../../static/my/icon/cards@2x.png" data-event-opts="{{[['^click',[['onCards']]]]}}" bind:click="__e" bind:__l="__l"></uni-item><uni-item vue-id="{{('6')+','+('1')}}" title="我的亲友" thumb="../../static/my/ico