<view class="container"><view class="container-header"><image src="../../../static/baner1@2x.png"></image></view><search class="search" vue-id="1" title="搜索订单" bind:__l="__l"></search><view class="container-button"><block wx:for="{{container}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="container-item" id="{{index}}" data-event-opts="{{[['tap',[['onOrder',['$event']]]]]}}" bindtap="__e"><image src="{{item.imgSrc}}"></image><text>{{item.title}}</text></view></block></view><view class="container-bottom"><view data-event-opts="{{[['tap',[['onMyInfo',['$event']]]]]}}" class="bottom-left" bindtap="__e"><image src="../../../static/business/sy-h@2x.png"></image><text>首页</text></view><view data-event-opts="{{[['tap',[['onMy',['$event']]]]]}}" class="bottom-left" bindtap="__e"><image src="../../../static/business/wd@2x.png"></image><text>我的</text></view></view><block wx:if="{{isShowPopup}}"><view class="popup"><view class="popup-content"><image src="../../../static/faults/close@2x.png"></image><text class="order-num">{{"您有"+orderNum+"个新的维修订单"}}</text><text class="order-type">{{orderType}}</text><view class="popup-content-button"><view data-event-opts="{{[['tap',[['bindLeft',['$event']]]]]}}" class="button-left" bindtap="__e">休息一下</view><view data-event-opts="{{[['tap',[['bindRight',['$event']]]]]}}" class="button-right" bindtap="__e">去抢单</view></view></view><view class="mask"></view></view></block></view>