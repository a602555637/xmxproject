
.button-class::after {
	border: none;
}
.button-class text {
	font-size: 24rpx;
	color: #000;
	position: relative;
	bottom: 14rpx;
}
.button-class image {
	width: 50rpx;
	height: 50rpx;
}
.button-class {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
}
.container-left {
	font-size: 24rpx;
	margin-left: 24rpx;
}
.container-left image {
	width: 50rpx;
	height: 50rpx;
	/* margin-top: 16upx; */
}
.container {
	position: fixed;
	bottom: 0rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	width: 750rpx;
	height: 98rpx;
	display: flex;
	background: #fff;
	padding-top: 10rpx;
	padding-bottom: 10rpx;
	border-top: 1px solid #F3F3F3;
}
.container-left {
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
}
.container-right,
.container-left {
	width: 350rpx;
}

